@model GOT.Models.SpisTrasPunktowanych

@{
    ViewData["Title"] = "Usuń";
}
<div class="table-wrapper">
    <h1>Usuwanie</h1>

    <h3>Czy jesteś pewien, że chcesz usunąć ten spis tras punktowanych?</h3>
    <div>
        <h4>Spis Tras Punktowanych</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.ValidFrom)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ValidFrom)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Track)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Track.Trail)
            </dd>
        </dl>

        <form class="form__margin--top" asp-action="Delete">
            <input type="hidden" asp-for="IdS" />
            <input type="submit" value="Usuń" class="btn btn-danger" /> |
            <a asp-action="Index">Powrót do listy</a>
        </form>
    </div>
</div>